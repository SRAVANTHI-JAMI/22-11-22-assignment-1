# program for students data
sno=int(input('enter sno:'))
sname=input('enter sname:')
sgroup=input('enter sgroup:')
sgender=input('enter sgender:')
sphno=int(input('enter sphno:'))
saddress=input('enter saddress:')
smarks =int(input('enter smarks'))
print('sno',sno)
print('sname',sname)
print('sgroup',sgroup)
print('sgender',sgender)
print('sphno',sphno)
print('saddress',saddress)
print('smarks',smarks)
